<h1>Listing riyu_okunais</h1>

<table>
  <thead>
    <tr>
      <th>Atai</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @riyu_okunais.each do |riyu_okunai| %>
      <tr>
        <td><%= riyu_okunai.atai %></td>
        <td><%= link_to 'Show', riyu_okunai %></td>
        <td><%= link_to 'Edit', edit_riyu_okunai_path(riyu_okunai) %></td>
        <td><%= link_to 'Destroy', riyu_okunai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Riyu okunai', new_riyu_okunai_path %>
